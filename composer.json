{
  "name": "culturegr/presenter",
  "version": "1.0.1",
  "description": "Eloquent Model Presenter for Laravel applications",
  "license": "MIT",
  "authors": [
    {
      "name": "Iraklis Georgas",
      "email": "igeorgas@cutlure.gr",
      "homepage": "https://culture.gr"
    }
  ],
  "homepage": "https://github.com/culturegr/presenter",
  "keywords": [
    "Laravel",
    "Presenter"
  ],
  "require": {
    "illuminate/support": "^6.5|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^5.1",
    "sempro/phpunit-pretty-print": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "CultureGr\\Presenter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CultureGr\\Presenter\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CultureGr\\Presenter\\PresenterServiceProvider"
      ]
    }
  }
}
